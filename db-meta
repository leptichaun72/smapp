tbl brokers
id primary
name varchar(25)
date_created datetime

tbl stocks

id primary
name varchar(100)
ticker varchar(25)
entry_price double
exit_price double
pricetgt_low double
pricetgt_median double
pricetgt_high double
pricetgt_average double

tbl lots

id primary
stock_id int
qty double
price double
date_created datetime
date_modified datetime
date_filled datetime

